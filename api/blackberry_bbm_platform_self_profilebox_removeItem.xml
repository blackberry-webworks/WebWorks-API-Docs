<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE reference PUBLIC "-//IXIA//DTD IXIA DITA Composite//EN" "../../system/dtd/ixia/IxiaDitabase.dtd">
<reference id="blackberry_bbm_platform_self_profilebox_removeItem" xml:lang="en-us">
    <title><apiname>removeItem</apiname></title>
    <shortdesc><i>Removes an item.</i></shortdesc>
    <refbody>
        <section>
            <title>Synopsis:</title>
                <pre scale="80">void removeItem(item, onSuccess, onError)</pre>
        </section>
        <section>
            <title>Parameters:</title>
            <dl>
                <dlentry>
                    <dt><varname>item</varname> {<xref format="html" href="blackberry_bbm_platform_self_profilebox_ProfileBoxItem.html" scope="peer">ProfileBoxItem</xref>}</dt>
                    <dd><p>The item to remove.</p></dd>
                </dlentry>
                <dlentry>
                    <dt><varname>onSuccess</varname> {Function}</dt>
                    <dd><p>Invoked when the item has been removed.</p>
                        <dl>
                            <dlentry>
                                <dt><varname>item</varname> {<xref format="html" href="blackberry_bbm_platform_self_profilebox_ProfileBoxItem.html" scope="peer">ProfileBoxItem</xref>}</dt>
                                <dd><p>The item that was removed.</p></dd>
                            </dlentry>
                        </dl>
                    </dd>
                </dlentry>
                <dlentry>
                    <dt><varname>onError</varname> {Function}</dt>
                    <dd><p>Invoked when there was an error removing the item.</p>
                        <dl>
                            <dlentry>
                                <dt><varname>error</varname> {String}</dt>
                                <dd><p>Contains the error message.</p></dd>
                            </dlentry>
                        </dl>
                    </dd>
                </dlentry>
            </dl>
        </section>
    </refbody>
</reference>
