<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE reference PUBLIC "-//IXIA//DTD IXIA DITA Composite//EN" "../../system/dtd/ixia/IxiaDitabase.dtd">
<reference id="blackbery_sensors_deviceholster" xml:lang="en-us">
    <title><apiname>deviceholster()</apiname></title>
    <shortdesc><i>This sensor returns the status of the holster sensor.</i></shortdesc>
    <refbody>
        <section>
            <title>Synopsis:</title>
            <pre scale="80">deviceholster(data)</pre>
        </section>
        <section>
            <title>Parameters:</title>
            <dl>
                <dlentry>
                    <dt><varname>data</varname> {Object}</dt>
                    <dd><p>The object containing the sensor data. </p>
                    <dl>
                    	<dlentry>
                    		<dt><varname>data.value</varname> {Number}</dt>
                    		<dd><p>1 if device is holstered, 0 otherwise.</p></dd>
                    	</dlentry>
                    </dl></dd>
                </dlentry>
            </dl>
        </section>
    </refbody>
</reference>
