<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE reference PUBLIC "-//IXIA//DTD IXIA DITA Composite//EN" "../../system/dtd/ixia/IxiaDitabase.dtd">
<reference id="blackberry_payment_PaymentError_errorID" xml:lang="en-us">
    <title><apiname>errorID</apiname></title>
    <shortdesc><i>The reference number associated with the specific error in corresponding to the following values.</i></shortdesc>
    <refbody>
        <section>
            <title>Synopsis:</title>
            <pre scale="80">blackberry_payment_PaymentError_errorID {Number}</pre>
	        <p><ul>
		        <li>Unexpected Application Error = -1</li>
		        <li>User Cancelled = 1</li>
		        <li>Payment System Busy = 2</li>
		        <li>General Payment System Error  = 3</li>
		        <li>Digital Good not Found = 4</li>
	        </ul></p>
        </section>
    </refbody>
</reference>
